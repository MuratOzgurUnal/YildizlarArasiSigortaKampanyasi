{
    "builds": [
      {
        "src": "assets/js/config.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/$1"
      }
    ],
    "build": {
      "env": {
        "GOOGLE_API_KEY": "@google_api_key",
        "GEMINI_API_KEY": "@gemini_api_key"
      }
    }
  }